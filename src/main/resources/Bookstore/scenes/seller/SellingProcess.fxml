<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Polygon?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.HBox?>
<VBox fx:id="container" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml"
      fx:controller="Bookstore.scenes.seller.SellingProcess">
    <AnchorPane>
        <HBox AnchorPane.topAnchor="2.0" AnchorPane.rightAnchor="2.0">
            <Region HBox.hgrow="ALWAYS"/>
            <Button text="Back" fx:id="backButton" onAction="#goBack"/>
        </HBox>
    </AnchorPane>
    <HBox fx:id="container2" alignment="CENTER"/>
    <VBox fx:id="contentArea"/>
    <Label fx:id="selectedCategoryLabel" text="Selected Category: " />
    <Label fx:id="selectedConditionLabel" text="Selected Condition: " />
    <Label fx:id="selectedPriceLabel" text="Inputted Price: " />
    <Label fx:id="offeredPriceLabel" text="Offered Price: " />
    <Label fx:id="bookTitleLabel" text="Title: " />
    <Label fx:id="bookDescriptionLabel" text="Description: " />
    <Label fx:id="bookAuthorLabel" text="Author: " />


    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>


</VBox>
