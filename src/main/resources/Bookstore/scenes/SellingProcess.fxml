<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Polygon?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.layout.HBox?>
<VBox fx:id="container" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="Bookstore.scenes.SellingProcess">
    <HBox fx:id="container2" alignment="CENTER"/>
    <VBox fx:id="contentArea"/>
    <Label fx:id="selectedCategoryLabel"/>
    <Label fx:id="selectedConditionLabel"/>
    <Label fx:id="selectedPriceLabel"/>
    <Label fx:id="offeredPriceLabel"/>


    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>


</VBox>
